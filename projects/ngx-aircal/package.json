{
  "name": "ngx-aircal",
  "version": "0.1.0",
  "peerDependencies": {
    "@angular/common": "^7.0.0",
    "@angular/core": "^7.0.0"
  },
  "dependencies": {
    "date-fns": "^2.0.0-alpha.27"
  },
  "keywords": [
    "angular",
    "angular2",
    "angular4",
    "angular5",
    "calendar",
    "calendar plugin",
    "date range",
    "date range plugin",
    "angular date range",
    "angular calendar"
  ],
  "description": "![version](https://img.shields.io/npm/v/ngx-aircal.svg?colorB=00a699) ![license](https://img.shields.io/github/license/cswarb/ngx-aircal.svg?colorB=00a699) ![weight](https://img.shields.io/bundlephobia/minzip/ngx-aircal.svg?colorB=00a699) ![dependencies](https://img.shields.io/david/cswarb/ngx-aircal.svg?colorB=00a699)",
  "main": "",
  "devDependencies": {
    "cpx": "^1.5.0",
    "scss-bundle": "^2.4.0"
  },
  "scripts": {
    "build-ci": "npm run build-lib",
    "test-ci": "ng test ngx-aircal --watch=false --browsers=ChromeHeadless",
    "build-lib": "ng build ngx-aircal && npm run build-mylib-styles",
    "build-mylib-styles": "cpx \"./src/lib/style/**/*\" \"../../dist/ngx-aircal/style\" ",
    "cp-mylib-assets": "cpx \"./src/assets/**/*\" \"../../dist/ngx-aircal/assets\"",
    "publish": "cd ../../dist/ngx-aircal && npm version && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cswarb/ngx-aircal.git"
  },
  "author": "Chris Swarbrick <chris.swarb@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/cswarb/ngx-aircal#readme",
  "bugs": {
    "url": "https://github.com/cswarb/ngx-aircal/issues"
  }
}
