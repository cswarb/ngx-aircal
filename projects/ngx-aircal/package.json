{
  "name": "ngx-aircal",
  "version": "1.2.0",
  "peerDependencies": {
    "@angular/common": "^7.0.0",
    "@angular/core": "^7.0.0"
  },
  "dependencies": {
    "date-fns": "2.0.0-alpha.27"
  },
  "keywords": [
    "angular",
    "angular 2",
    "angular 4",
    "angular 5",
    "angular 6",
    "angular 7",
    "calendar",
    "calendar plugin",
    "calendar range plugin",
    "date range",
    "date range plugin",
    "angular date range",
    "angular calendar"
  ],
  "description": "ngx-aircal is a modern, configurable and lightweight Angular date range picker.",
  "main": "",
  "devDependencies": {
    "cpx": "^1.5.0",
    "scss-bundle": "^2.4.0"
  },
  "scripts": {
    "build-ci": "npm run build-lib",
    "test-ci": "ng test ngx-aircal --watch=false --browsers=ChromeHeadless",
    "build-lib": "ng build ngx-aircal && npm run build-mylib-styles && npm run cp-mylib-images",
    "build-mylib-styles": "cpx \"./src/lib/style/**/*\" \"../../dist/ngx-aircal/style\" ",
    "cp-mylib-assets": "cpx \"./src/assets/**/*\" \"../../dist/ngx-aircal/assets\"",
    "cp-mylib-images": "cpx \"./aircal.png\" \"../../dist/ngx-aircal/\"",
    "publish": "cd ../../dist/ngx-aircal && npm version && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cswarb/ngx-aircal.git"
  },
  "author": "Chris Swarbrick <chris.swarb@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/cswarb/ngx-aircal#readme",
  "bugs": {
    "url": "https://github.com/cswarb/ngx-aircal/issues"
  }
}
